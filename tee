SELECT SUM( NUMERIC_FIELD ) FROM BUCKET_NAME WHERE NUMERIC_FIELD < NUMERIC_VALUE GROUP BY NUMERIC_FIELD_LIST, STRING_FIELD_LIST, DATETIME_FIELD_LIST;
SELECT COUNT( DISTINCT STRING_FIELD_LIST ) FROM BUCKET_NAME WHERE DATETIME_FIELD < DATETIME_VALUES GROUP BY NUMERIC_FIELD_LIST, STRING_FIELD_LIST;
SELECT MIN( NUMERIC_FIELD ) FROM BUCKET_NAME WHERE (STRING_FIELD < STRING_VALUES) OR (((DATETIME_FIELD IS NULL) AND (STRING_FIELD != STRING_VALUES)) AND (((STRING_FIELD IS NOT NULL) AND (STRING_FIELD IS NULL)) OR (STRING_FIELD NOT BETWEEN LOWER_BOUND_VALUE and UPPER_BOUND_VALUE)));
SELECT MAX( NUMERIC_FIELD ) FROM BUCKET_NAME WHERE (DATETIME_FIELD IS NOT NULL) OR (DATETIME_FIELD IS NULL);
SELECT MIN( NUMERIC_FIELD ) FROM BUCKET_NAME WHERE (STRING_FIELD <= STRING_VALUES) OR (STRING_FIELD >= STRING_VALUES) GROUP BY NUMERIC_FIELD_LIST, STRING_FIELD_LIST, DATETIME_FIELD_LIST;
SELECT AVG( DISTINCT DATETIME_FIELD ) FROM BUCKET_NAME WHERE (NUMERIC_FIELD IS NULL) AND (NUMERIC_FIELD < NUMERIC_VALUE) GROUP BY NUMERIC_FIELD_LIST;
SELECT AVG( DISTINCT NUMERIC_FIELD ) FROM BUCKET_NAME WHERE NUMERIC_FIELD IN [ LIST ];
SELECT SUM( NUMERIC_FIELD ) FROM BUCKET_NAME WHERE STRING_FIELD IS NOT NULL GROUP BY DATETIME_FIELD_LIST;
SELECT * FROM BUCKET_NAME WHERE ((((DATETIME_FIELD <= DATETIME_VALUES) OR (DATETIME_FIELD != DATETIME_VALUES)) OR (DATETIME_FIELD = DATETIME_VALUES)) OR (DATETIME_FIELD IN [ LIST ])) AND (NOT ((NUMERIC_FIELD IS NULL) AND (NUMERIC_FIELD IS NOT NULL))) GROUP BY NUMERIC_FIELD_LIST, STRING_FIELD_LIST, DATETIME_FIELD_LIST;
SELECT MAX( NUMERIC_FIELD ) FROM BUCKET_NAME WHERE ((DATETIME_FIELD >= DATETIME_VALUES) AND ((DATETIME_FIELD IN [ LIST ]) OR (NUMERIC_FIELD IS NOT NULL))) OR ((((NOT (DATETIME_FIELD IS NULL)) OR (NUMERIC_FIELD != NUMERIC_VALUE)) AND ((NUMERIC_FIELD = NUMERIC_VALUE) OR ((NUMERIC_FIELD <= NUMERIC_VALUE) AND ((NUMERIC_FIELD >= NUMERIC_VALUE) OR ((NOT (NUMERIC_FIELD <= NUMERIC_VALUE)) AND (NUMERIC_FIELD != NUMERIC_VALUE)))))) AND (DATETIME_FIELD IS NOT NULL)) GROUP BY DATETIME_FIELD_LIST;
